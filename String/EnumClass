import java.lang.Enum;
import java.util.Random;
public class enumClass {
    /**
     * 枚举
     * */
    enum enums {
        A,B,C
    }
    //main方法
    public static void main(String[] args){
        switch (enums.A){
            case A:
                System.out.println("枚举类型："+enums.A);
                break;
            case B:
                System.out.println("枚举类型："+enums.B);
                break;
        }
    }
}
